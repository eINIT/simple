<module>
 <v-entranced
  name="EntranceD"
  provides="x11:displaymanager:entranced">

  <execute id="enable" code="entranced" />
  <execute id="disable" code="killall entranced" />

  <need-files s="entranced:X:/tmp/.:/home/." />
  <environment DISPLAY=":0.0" HOME="/root" />
 </v-entranced>
</module>
